@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.TextPage>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = "Master.cshtml";
}

<section class="page-title">
	<div class="container">
		<div class="row">
			<div class="col-12">
				<h1>@(!string.IsNullOrWhiteSpace(Model.PageTitle) ? Model.PageTitle : Model.Name)</h1>
			</div>
		</div>
	</div>
</section>